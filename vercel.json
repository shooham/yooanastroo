{
  "version": 2,
  "name": "yooanastro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "nodeVersion": "18.x",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
